set number
set laststatus=2
syntax on
set relativenumber
set tabstop=4
set autoindent
set mouse=a
:map <C-t> :NERDTree
:map! <S-z> <C-n>
map <C-c> "+y
map <C-v> "+P
call plug#begin()
	Plug 'scrooloose/nerdtree'
	Plug 'dense-analysis/ale'
	Plug 'altercation/vim-colors-solarized'
	Plug 'drewtempelmeyer/palenight.vim'
call plug#end()
set background=dark
colorscheme palenight

